/**
* Need to install npm install babel-preset-env --save-dev.
*/

{
  "presets": [
    ["env", {  
      "targets": {
        "browsers": [
          "Chrome >= 52",
          "FireFox >= 44",
          "Safari >= 7",
          "Explorer 11",
          "last 4 Edge versions"
        ]
        },
        "useBuiltIns": true,
        "modules": false
      }
    ],
    "stage-1"
  ] ,
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    ["transform-class-properties"]
  ]
}

